{
	"remote": {
		"host": "sapeka.vtexcommercebeta.com.br",
		"port": 80
	},
	"proxy": {
		"host": "sapeka.vtexlocal.com.br",
		"port": 80
	},
	"server": {
		"host": "localhost",
		"port": 9001
	},
	"headers": {
    "X-VTEX-Environment": "beta"
	},
	"bounces":[
		"style/.*",
		"index.html",
        "minicart.html"
	],
	"mapping": true,
	"mappings": {
	    ".*portal-minicart-with-template.min.js": "build/js/portal-minicart-with-template.dev.js",
	    ".*portal-sku-selector-with-template.min.js.*": "build/js/portal-sku-selector-with-template.dev.js"
	}
}
