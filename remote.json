{
	"remote": {
		"host": "casaamerica.com.adsafasbr",
		"port": 80
	},
	"proxy": {
		"host": "localhost",
		"port": 80
	},
	"server": {
		"host": "localhost",
		"port": 9001
	},
	"headers": {
    "X-VTEX-Environment": "beta"
	},
	"bounces":[
		"style/.*",
		"index.html",
    "minicart.html"
	],
	"mapping": true,
	"mappings": {
	    ".*portal-minicart-with-template.min.js": "build/js/portal-minicart-with-template.js",
	    ".*portal-sku-selector-with-template.min.js": "build/js/portal-sku-selector-with-template.js",
	    ".*portal-template-as-modal.min.js.*": "build/js/portal-template-as-modal.js",
      "dust-core-2.0.0.min.js": "build/lib/dust-core-2.0.0.min.js",
      "vtex-utils.min.js": "build/lib/vtex-utils.min.js"
	}
}
